// 1st problem of blind 75 leetode soluction
*******************************************

// TWO SUM LEET CODE PROBLEM
**************************************

#include<iostream>
using namespace std;
void  ans(int arr[], int n, int target){
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[i]+arr[j]==target){
                cout<<"The final answer is  "<<i<<" "<<" and "<<j<<endl;
            }
        }
    }
}
int main(){
    int n;
    cin>>n;
    
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int target;
    cout<<"enter the target!!"<<endl;
    cin>>target;
    ans(arr,n,target);
}
